{
  "name": "@jsk-std/cli",
  "version": "0.1.0",
  "author": "lynnkoo",
  "description": "",
  "bin": {
    "mono-cli": "bin/index.js"
  },
  "scripts": {
    "start": "tsc -p tsconfig.prod.json --watch",
    "build": "tsc -p tsconfig.prod.json ",
    "clean": "rm -rf dist",
    "test": "npm run build && npm run clean && npm run test:create",
    "test:boot": "mono-cli boot --dir dist/test-project",
    "test:sync": "mono-cli sync --dir dist/test-project",
    "test:add": "mono-cli add packages pkg1,pkg2,pkg3 --dir dist/test-project",
    "test:create": "mono-cli create dist/test-project",
    "test:dep": "mono-cli dep jest,pkg1 pkg1,pkg3 --dir dist/test-project",
    "test:down": "mono-cli download --force",
    "test:depd": "mono-cli dep @test-project/pkg1,jest,pkg1,pkg2 pkg1 --del --dir dist/test-project"
  },
  "homepage": "https://github.com/jsk-stuido/jsk-std#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jsk-stuido/jsk-std.git"
  },
  "bugs": {
    "url": "https://github.com/jsk-stuido/jsk-std/issues"
  },
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "jest": "^26.0.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.1.0",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "@jsk-std/io": "^0.1.1",
    "@jsk-std/x": "^0.1.1",
    "download-git-repo": "^3.0.2",
    "readline-sync": "^1.4.10"
  }
}
